    "celebrities['date'] = pd.to_datetime(celebrities['date'])\n",
    "\n",
    "covid19_start = datetime.datetime(year=2020, month=3, day=1)\n",
    "covid19_end = datetime.datetime(year=2021, month=5, day=5) #we do not actually end up using this\n",
    "covid19_timestamps = pd.Series(pd.date_range(start=covid19_start, end=covid19_end))\n",
    "#print(covid19_timestamps)\n",
    "\n",
    "election_start = datetime.datetime(year=2020, month=10, day=15)\n",
    "election_end = datetime.datetime(year=2020, month=11, day=8)\n",
    "election_timestamps = pd.Series(pd.date_range(start=election_start, end=election_end))\n",
    "#print(election_timestamps)\n",
    "\n",
    "blm_start = datetime.datetime(year=2020, month=5, day=1)\n",
    "blm_end = datetime.datetime(year=2020, month=8, day=1)\n",
    "blm_timestamps = pd.Series(pd.date_range(start=blm_start, end=blm_end))"
   ]
