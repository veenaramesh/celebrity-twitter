    "election_tweets = celebrities.loc[(celebrities['date'] > election_start)]\n",
    "election_tweets = election_tweets.loc[celebrities['date'] < election_end]\n",
    "election_tweets.reset_index(inplace=True)\n",
    "\n",
    "no_election_tweets = pd.concat([celebrities.loc[celebrities['date'] < election_start], \n",
    "                                celebrities.loc[celebrities['date'] > election_end]],\n",
    "                               ignore_index=True)"
